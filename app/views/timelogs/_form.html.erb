<%= simple_form_for(@timelog) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :rate %>
    <%= f.input :date %>
    <%= f.input :nature_work %>
    <%= f.input :hours_worked %>
    <%= f.association :equipment %>
    <%= f.input :total %>
    <%= f.input :employee %>
    <%= f.association :work_order %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
